var tigger={char: "Tigger"}
var pooh={char: "Winnie the Pooh"}
tigger.north=pooh;
pooh.south=tigger;
var piglet={char:"Piglet"}
piglet.east=tigger.north;
tigger.north.west = piglet;
var bees={char:"Bees"}
bees.west=tigger.north;
tigger.north.east=bees;
var chris={char:"Christopher Robert"}
chris.south=pooh.north;
pooh.north=chris;
var owl={char:"Owl"}
owl.east=pooh.north;
pooh.north.west=owl;
var rabbit={char:"Rabbit"}
rabbit.west=pooh.north;
pooh.north.east=rabbit;
var gopher={char:"Gopher"}
gopher.west=pooh.north.west;
pooh.north.west.west=gopher;
var kanga={char:"Kanga"}
chris.north=kanga;
kanga.south=chris;
var eyeore={char:"Eyeore"}
kanga.north=eyeore;
eyeore.south=kanga;
var heff={char:"Heff"}
heff.west=kanga.north;
kanga.north.east=heff;
